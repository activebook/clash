// Convert CIDR notation to IP range
function cidrToRange(cidr) {
    var parts = cidr.split("/");
    var ip = parts[0];
    var mask = parseInt(parts[1], 10);
    var ipParts = ip.split(".");
    var ipNum = (parseInt(ipParts[0], 10) << 24) +
        (parseInt(ipParts[1], 10) << 16) +
        (parseInt(ipParts[2], 10) << 8) +
        parseInt(ipParts[3], 10);
    var maskNum = -1 << (32 - mask);
    var start = ipNum & maskNum;
    var end = start + Math.pow(2, (32 - mask)) - 1;
    return [start, end];
}

// Check if IP is in range
function ipInRange(ip, range) {
    var ipParts = ip.split(".");
    var ipNum = (parseInt(ipParts[0], 10) << 24) +
        (parseInt(ipParts[1], 10) << 16) +
        (parseInt(ipParts[2], 10) << 8) +
        parseInt(ipParts[3], 10);
    return ipNum >= range[0] && ipNum <= range[1];
}

// DNS lookup function
function dnsLookup(host) {
    try {
        return dnsResolve(host);
    } catch (e) {
        return null;
    }
}

// Helper function to check for localhost
function isLocalHost(host) {
    return host === "localhost" ||
        host === "127.0.0.1" ||
        host.match(/^localhost\./) ||
        host.match(/^127\.0\.0\.1/);
}

// Main function to determine proxy settings
function FindProxyForURL(url, host) {
    // Handle non-hostnames (IP addresses)
    if (isPlainHostName(host) || /^(\d+\.){3}\d+$/.test(host)) {
        // Check if IP is in Chinese ranges
        for (var i = 0; i < chinaIPRanges.length; i++) {
            var range = cidrToRange(chinaIPRanges[i]);
            if (ipInRange(host, range)) {
                return direct;
            }
        }
        return proxy;
    }

    // Handle localhost
    if (isLocalHost(host)) {
        return direct;
    }

    // Check critical Chinese domains first (hybrid approach)
    if (criticalChineseDomains[host] === 1) {
        return direct;
    }

    // Check for subdomains of critical Chinese domains
    for (var domain in criticalChineseDomains) {
        if (host.endsWith("." + domain) || host === domain) {
            return direct;
        }
    }

    // Resolve hostname to IP
    var resolvedIP = dnsLookup(host);
    if (resolvedIP) {
        // Check if resolved IP is in Chinese ranges
        for (var i = 0; i < chinaIPRanges.length; i++) {
            var range = cidrToRange(chinaIPRanges[i]);
            if (ipInRange(resolvedIP, range)) {
                return direct;
            }
        }
    }

    // Default to proxy for non-Chinese sites
    return proxy;
}